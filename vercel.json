{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "routes": [
    { "src": "/css/(.*)", "dest": "/css/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/components/(.*)", "dest": "/components/$1" },
    { "src": "/contexts/(.*)", "dest": "/contexts/$1" },
    { "src": "/(.*\\.(json|js|css|png|jpg|svg|html))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
